<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/_81694f</item>
        <item name="colorPrimaryDark">@color/_81694f</item>
        <item name="colorAccent">@color/_81694f</item>
    </style>
    <style name="ThemeSplash" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:background">@mipmap/first_draw</item>
        <item name="colorPrimary">@color/_dcd8cc</item>
        <item name="colorPrimaryDark">@color/_dcd8cc</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">false</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="me_btn_next">
        <item name="android:layout_width">@dimen/rw_spacing_size_18</item>
        <item name="android:layout_height">@dimen/rw_spacing_size_23</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="line_width_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">#d9d9d9</item>
    </style>

    <style name="home_state">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_gravity">bottom</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:button">@null</item>
        <item name="android:drawablePadding">@dimen/rw_spacing_size_20</item>
        <item name="android:gravity">bottom|center_horizontal</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/rw_text_size_25</item>
        <item name="android:padding">@dimen/rw_spacing_size_5</item>
    </style>
    <style name="MyCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/home_top_bottomback</item>
    </style>

    <style name="HomeCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/home_btn_check</item>
    </style>


</resources>
